oken';\n\nif(localStorage.token){\n  setAuthToken(localStorage.token);\n};\n\nfunction App() {\n  return (\n    <AuthState>\n      <ContactState>\n        <AlertState>\n          <Router>\n            <Fragment>\n              <Navbar />\n              <div className=\"container\">\n                <Alerts />\n                <Switch>\n                  <Route exact path='/' component={Home} />\n                  <Route exact path='/about' component={About} />\n                  <Route exact path='/register' component={Register} />\n                  <Route exact path='/login' component={Login} />\n                </Switch>\n              </div>\n            </Fragment>\n          </Router>\n        </AlertState>\n      </ContactState>\n    </AuthState>\n  );\n}\n\nexport default App;\n","/Users/rahulrinwa/Desktop/contact-keeper/client/src/reportWebVitals.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/components/layout/Navbar.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/components/pages/About.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/components/pages/Home.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/context/contact/ContactState.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/context/contact/contactContext.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/context/contact/contactReducer.js",["108"],"import {\n  ADD_CONTACT,\n  DELETE_CONTACT,\n  SET_CURRENT,\n  CLEAR_CURRENT,\n  UPDATE_CONTACT,\n  FILTER_CONTACTS,\n  CLEAR_FILTER,\n} from '../types';\n\nexport default (state, action) => {\n    switch(action.type){\n        default:\n            return state\n\n        case ADD_CONTACT:\n            return{\n                ...state,\n                contacts: [...state.contacts, action.payload]\n            }\n\n        case DELETE_CONTACT:\n            return{\n                ...state,\n                contacts: state.contacts.filter(contact => contact.id !== action.payload)//return all contacts that are not the cuurrent id that's in the payload\n            }\n\n        case SET_CURRENT:\n            return{\n                ...state,\n                current: action.payload\n            }\n\n        case CLEAR_CURRENT:\n            return{\n                ...state,\n                current: null\n            }\n\n        case UPDATE_CONTACT:\n            return{\n                ...state,\n                contacts: state.contacts.map(contact => contact.id === action.payload.id ? action.payload : contact) //action.payload is the entire contact\n            }\n\n        case FILTER_CONTACTS:\n            return{\n                ...state,\n                filtered: state.contacts.filter(contact => {\n                    const regex = new RegExp(`${action.payload}`, 'gi');\n                    return contact.name.match(regex) || contact.email.match(regex);\n                })\n            }\n\n        case CLEAR_FILTER:\n            return{\n                ...state,\n                filtered: null\n            }\n    }\n}","/Users/rahulrinwa/Desktop/contact-keeper/client/src/context/types.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/components/contacts/Contacts.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/components/contacts/ContactItem.js",[],["109","110"],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/components/contacts/ContactForm.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/components/contacts/ContactFilter.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/context/auth/AuthState.js",["111"],"import React, { useReducer } from 'react';\nimport axios from 'axios';\nimport AuthContext from './authContext';\nimport authReducer from './authReducer';\nimport setAuthToken from '../../utils/setAuthToken';\n\nimport {\n  REGISTER_SUCCESS,\n  REGISTER_FAIL,\n  USER_LOADED,\n  AUTH_ERROR,\n  LOGIN_SUCCESS,\n  LOGIN_FAIL,\n  LOGOUT,\n  CLEAR_ERRORS\n} from '../types'; \n\nconst AuthState = props => {\n    const initialState = {\n        token: localStorage.getItem('token'),\n        isAuthenticated: null,\n        loading: true,\n        user: null,\n        error: null\n    };\n\n    //state allows us to access anything in our state and dispatch allows us to dispatch objects to the reducer\n    const [state, dispatch] = useReducer(authReducer, initialState);\n\n    //all the actions\n\n    //load user\n    const loadUser = async () => {\n        // load token into global header\n        if(localStorage.token){\n            setAuthToken(localStorage.token);\n        }\n\n        try {\n            const res = await axios.get('/api/auth');\n\n            dispatch({type: USER_LOADED, payload: res.data})\n        } catch (error) {\n            dispatch({ type: AUTH_ERROR })\n        }\n    };\n\n    //register user\n    const register = async formData => {\n        const config = {\n            header: {\n                'Content-Type': 'application/json'\n            }\n        }\n\n        try {\n            const res = await axios.post('/api/users', formData, config);\n\n            dispatch({\n                type: REGISTER_SUCCESS,\n                payload: res.data  //res.data is the token\n            });\n\n            loadUser();\n        } catch (error) {\n            dispatch({\n                type: REGISTER_FAIL,\n                payload: error.response.data.msg\n            });\n        }\n    }\n\n    //login user\n    const login = async formData => {\n        const config = {\n            header: {\n                'Content-Type': 'application/json'\n            }\n        }\n\n        try {\n            const res = await axios.post('/api/auth', formData, config);\n\n            dispatch({\n                type: LOGIN_SUCCESS,\n                payload: res.data\n            });\n\n            loadUser();\n        } catch (error) {\n            dispatch({\n                type: LOGIN_FAIL,\n                payload: error.response.data.msg\n            });\n        }\n    }\n\n    //logout user\n\n    //clear errors\n    const clearErrors = () => {\n        dispatch({ type: CLEAR_ERRORS })\n    }\n\n    return (\n        <AuthContext.Provider\n        value={{\n            token: state.token,\n            isAuthenticated: state.isAuthenticated,\n            loading: state.loading,\n            user: state.user,\n            error: state.error,\n            register,\n            clearErrors,\n            loadUser,\n            login\n        }}>\n            { props.children }\n        </AuthContext.Provider>\n    )\n};\n\nexport default AuthState;","/Users/rahulrinwa/Desktop/contact-keeper/client/src/context/auth/authReducer.js",["112","113"],"import {\n  REGISTER_SUCCESS,\n  REGISTER_FAIL,\n  USER_LOADED,\n  AUTH_ERROR,\n  LOGIN_SUCCESS,\n  LOGIN_FAIL,\n  LOGOUT,\n  CLEAR_ERRORS\n} from '../types';\n\nexport default (state, action) => {\n    switch(action.type){\n        default:\n            return state\n\n        case REGISTER_SUCCESS:\n        case LOGIN_SUCCESS:\n            localStorage.setItem('token', action.payload.token);\n            return{\n                ...state,\n                ...action.payload, //put the token in state \n                isAuthenticated: true,\n                loading: false\n            };\n\n        case REGISTER_FAIL:\n        case AUTH_ERROR:\n        case LOGIN_FAIL:\n            localStorage.removeItem('token');\n            return{\n                ...state,\n                token: null,\n                isAuthenticated: false,\n                loading: false,\n                user: null,\n                error: action.payload\n            }\n\n        case CLEAR_ERRORS:\n            return{\n                ...state,\n                error: null\n            }\n\n        case USER_LOADED:\n            return{\n                ...state,\n                isAuthenticated: true,\n                loading: false,\n                user: action.payload\n            }\n    }\n}","/Users/rahulrinwa/Desktop/contact-keeper/client/src/context/auth/authContext.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/components/auth/Register.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/components/auth/Login.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/context/alert/AlertState.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/context/alert/alertContext.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/context/alert/alertReducer.js",["114"],"import { SET_ALERT, REMOVE_ALERT } from '../types';\n\nexport default (state, action) => {\n  switch (action.type) {\n    case SET_ALERT:\n      return [...state, action.payload];\n\n    case REMOVE_ALERT:\n      return state.filter(alert => alert.id !== action.payload);\n      \n    default:\n      return state;\n  }\n};","/Users/rahulrinwa/Desktop/contact-keeper/client/src/components/layout/Alerts.js",[],"/Users/rahulrinwa/Desktop/contact-keeper/client/src/utils/setAuthToken.js",[],{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","severity":1,"message":"120","line":9,"column":8,"nodeType":"121","messageId":"122","endLine":9,"endColumn":13},{"ruleId":"123","severity":1,"message":"124","line":11,"column":1,"nodeType":"125","endLine":61,"endColumn":2},{"ruleId":"115","replacedBy":"126"},{"ruleId":"117","replacedBy":"127"},{"ruleId":"119","severity":1,"message":"128","line":14,"column":3,"nodeType":"121","messageId":"122","endLine":14,"endColumn":9},{"ruleId":"119","severity":1,"message":"128","line":8,"column":3,"nodeType":"121","messageId":"122","endLine":8,"endColumn":9},{"ruleId":"123","severity":1,"message":"124","line":12,"column":1,"nodeType":"125","endLine":54,"endColumn":2},{"ruleId":"123","severity":1,"message":"124","line":3,"column":1,"nodeType":"125","endLine":14,"endColumn":3},"no-native-reassign",["129"],"no-negated-in-lhs",["130"],"no-unused-vars","'Alert' is defined but never used.","Identifi